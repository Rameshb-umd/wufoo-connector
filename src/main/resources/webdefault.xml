<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0">
  <!-- The following parameters are used to configure this web service. They 
    will be unique to your SysAid installation and will need to be changed. -->
  <context-param>
    <!-- The URL that POST requests are made to. The URL mapping for our 
      installation is below, but may vary across installations -->
    <param-name>SysAidURL</param-name>
    <param-value>https://example.com/webformsubmit?pageEncoding=utf-8</param-value>
  </context-param>
  <context-param>
    <!-- Your organization's account ID with SysAid. If you don't know it, 
      you can find it under the “About” menu in your help desk. -->
    <param-name>accountID</param-name>
    <param-value></param-value>
  </context-param>
    <context-param>
    <!-- SysAid uses this key to authorize requests to your help desk. It 
      will be a series of three hexadecimal numbers separated by colons. These 
      numbers can be positive or negative and are 8, 10, and 4 digits long, respectively. -->
    <param-name>formID</param-name>
    <param-value>0</param-value>
  </context-param>
</web-app>
